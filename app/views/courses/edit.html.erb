<%= link_to '<< Back to List', courses_path %>

<%= bootstrap_form_tag url: courses_path, method: 'put' do |f| %>
    <%= f.text_field :coursenumber, :placeholder => 'Course Number' %>

    <%= f.text_field :title, :placeholder => 'Title' %>

    <%= f.text_area :description, :placeholder => 'Description' %>

    <%= f.date_field :start_date, :placeholder => 'Start Date' %>

    <%= f.date_field :end_date, :placeholder => 'End Date' %>

    <%= f.text_field :email, :placeholder => 'Instructor Email' %>

    <%= f.submit 'Save' %>
<% end %>