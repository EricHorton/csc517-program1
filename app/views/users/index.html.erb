<div class="users index">
  <h2>Users</h2>
  <table class="listing" summary="User list">
    <tr class="header">
      <th>ID</th>
      <th>&nbsp;</th>
      <th>&nbsp;</th>
      <th>Name</th>
      <th>&nbsp;</th>
      <th>Email</th>
      <th>&nbsp;</th>
      <th>&nbsp;</th>
      <th>&nbsp;</th>
      <th>Type</th>
      <th>&nbsp;</th>
      <th>&nbsp;</th>
      <th>&nbsp;</th>
      <th>Actions</th>
    </tr>
    <% @users.each do |user| %>
        <tr>
          <td><%= user.id %></td>
          <th>&nbsp;</th>
          <th>&nbsp;</th>
          <td><%= user.name %></td>
          <th>&nbsp;</th>
          <td><%= user.email %></td>
          <th>&nbsp;</th>
          <th>&nbsp;</th>
          <th>&nbsp;</th>
          <td><%= user.type %></td>
          <th>&nbsp;</th>
          <th>&nbsp;</th>
          <th>&nbsp;</th>
          <td class="actions">
            <%= link_to 'Show', user_path(user) %>
            <%= link_to 'Delete', delete_user_path(user) %>
          </td>
        </tr>
    <% end %>
  </table>
</div>